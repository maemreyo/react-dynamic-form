{"code":"// src/features/form-renderer/components/FormLayout.tsx\nimport React, { useMemo } from 'react';\nimport { ThemeProvider } from 'styled-components';\nimport { defaultTheme } from '../../../theme';\nimport { getLayoutComponent } from '../../inputs/registry/LayoutRegistry';\nconst FormLayout = ({ onSubmit, children, className, formClassNameConfig, style, layout, layoutConfig, horizontalLabel, theme, }) => {\n    const mergedTheme = useMemo(() => {\n        return theme ? { ...defaultTheme, ...theme } : defaultTheme;\n    }, [theme]);\n    const handleSubmit = (event) => {\n        event.preventDefault();\n        onSubmit(event);\n    };\n    // Get the layout component from the registry\n    const LayoutComponent = getLayoutComponent(layout);\n    if (!LayoutComponent) {\n        console.warn(`No layout component found for type: ${layout}`);\n        return null; // Or return a default layout component\n    }\n    return (React.createElement(ThemeProvider, { theme: mergedTheme },\n        React.createElement(LayoutComponent, { onSubmit: handleSubmit, className: className, formClassNameConfig: formClassNameConfig, style: style, layoutConfig: layoutConfig, horizontalLabel: horizontalLabel }, children)));\n};\nexport default FormLayout;\n//# sourceMappingURL=FormLayout.js.map","references":["/Users/matthewngo/Documents/dev/projects/react-dynamic-form/node_modules/@types/react/index.d.ts","/Users/matthewngo/Documents/dev/projects/react-dynamic-form/node_modules/styled-components/dist/index.d.ts","/Users/matthewngo/Documents/dev/projects/react-dynamic-form/src/theme/index.ts","/Users/matthewngo/Documents/dev/projects/react-dynamic-form/src/features/dynamic-form/types.ts","/Users/matthewngo/Documents/dev/projects/react-dynamic-form/src/features/inputs/registry/LayoutRegistry.ts"],"map":"{\"version\":3,\"file\":\"FormLayout.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../src/features/form-renderer/components/FormLayout.tsx\"],\"names\":[],\"mappings\":